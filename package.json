{
  "name": "node-apn-forked",
  "description": "An interface to the Apple Push Notification service for Node.js",
  "version": "5.2.0",
  "author": "Andrew Naylor <argon@mkbot.net>",
  "keywords": [
    "apple",
    "push",
    "push notifications",
    "iOS",
    "apns",
    "notifications"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/parse-community/node-apn.git"
  },
  "dependencies": {
    "debug": "4.3.3",
    "jsonwebtoken": "8.5.1",
    "node-forge": "1.2.1",
    "verror": "1.10.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.40",
    "chai": "^4.3.4",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.1",
    "eslint": "^8.2.0",
    "mocha": "^8.3.2",
    "nyc": "15.1.0",
    "prettier": "^2.4.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "coverage": "node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint .",
    "prettier": "prettier --write {lib,test,examples,mock}/{**/*,*}.js index.js",
    "test": "node_modules/.bin/mocha"
  },
  "jshintConfig": {
    "node": true
  },
  "eslintConfig": {
    "ecmaVersion": 6,
    "env": {
      "es6": true,
      "node": true
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "license": "MIT"
}
